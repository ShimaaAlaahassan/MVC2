@{
    SelectList selectmanager = new SelectList(ViewBag.manager, "SSN", "Fname", Model.emp_m);
}
@model Department
<h2>Edit Department</h2>

@using (Html.BeginForm("SaveEdit", "Department", FormMethod.Post))
{
    <input name="Number" id="Number" hidden />
    <div>
        @Html.LabelFor(m=>m.Name)
        @Html.TextBoxFor(m=>m.Name,new {@class="form-control"})

    </div>
   <div>
       
 
    @Html.DropDownListFor(s=>s.emp_m,selectmanager,new {@class="form-control"})
   </div>

   <input  type="submit" value="Edit"/> 
}
